<h1>Settings</h1>

<h3>Websites</h3>
<% @websites.each do |website| %>
    <p><%= website.name %>(<%= website.url%>)</p>
<% end %>

<h3>Genders</h3>
<% @genders.each do |gender| %>
    <p><%= gender.name %></p>
<% end %>

<h3>Categories</h3>
<% @categories.each do |category| %>
    <p><%= category.name %></p>
<% end %>
